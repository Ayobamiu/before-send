{
  "manifest_version": 3,
  "name": "Send Check for Gmail",
  "version": "1.0.0",
  "description": "Stops you from sending emails you might regret.",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
